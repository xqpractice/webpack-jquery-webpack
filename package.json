{
  "name": "our-manager",
  "version": "0.1.0",
  "description": "Manager ours.",
  "main": "index.js",
  "engines": {
    "node": ">= 6"
  },
  "scripts": {
    "start": "NODE_ENV=production node config/server.js",
    "start:dev": "NODE_ENV=developer node config/server.dev.js",
    "start:prod": "NODE_ENV=production node config/server.prod.js",
    "build:clean": "rm -rf ./lib ./public/assets",
    "build:babel": "babel src -d lib --copy-files",
    "build:webpack": "webpack --config config/webpack/webpack.config.js",
    "build:watch": "watch 'yarn run build' ./src ./test",
    "build": "yarn run build:clean && NODE_ENV=production yarn run build:babel && NODE_ENV=production yarn run build:webpack",
    "test:lint": "eslint src",
    "test:unit": "mocha --compilers js:babel-register test/SpecHelper.js test/js/*-spec.js test/js/**/*-spec.js",
    "test:watch": "watch 'yarn run test' ./src ./test",
    "test": "yarn install && yarn run test:lint && yarn run test:unit",
    "release": "/bin/bash ./ci/git-release.sh"
  },
  "dependencies": {
    "bulma": "^0.4.2",
    "common-basic-validator": "^0.3.2",
    "es6-promise": "^4.1.0",
    "express": "^4.14.0",
    "express-common-controller": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "morgan": "^1.8.2",
    "node-fetch": "^1.7.1",
    "path": "^0.12.7",
    "promise-polyfill": "^6.0.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-highcharts": "^12.0.0",
    "react-router-dom": "^4.1.1",
    "simple-string-template": "^0.1.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.25.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.7.1",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "jsdom": "^9.11.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "null-loader": "^0.1.1",
    "react-addons-test-utils": "^15.4.2",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "watch": "^1.0.2",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-glob-entry": "^1.2.7",
    "webpack-hot-middleware": "^2.12.2",
    "when": "^3.7.8"
  },
  "keywords": [
    "Express",
    "React",
    "Karma",
    "Webpack"
  ],
  "author": "thomas",
  "license": "UNLICENSED"
}
